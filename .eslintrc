{
	"extends": ["airbnb", "prettier"],
	"plugins": ["prettier"],
	"settings": {
		"import/resolver": {
			"node": {
				"paths": ["src"]
			}
		}
	},
	"rules": {
		"prettier/prettier": "error",
		"no-console": 0,
		"import/extensions": 0,
		"react/jsx-indent": 0,
		"react/jsx-indent-props": 0,
		"react/jsx-uses-react": 2,
		"react/jsx-uses-vars": 2,
		"react/react-in-jsx-scope": 2
	}
}
