{
  "name": "gatsby-example-using-remark",
  "private": true,
  "description": "Gatsby example site that demonstrates gatsby-transformer-remark and its plugins.",
  "version": "1.0.0",
  "author": "Florian Kissling <sechskilo@gmail.com>",
  "devDependencies": {
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "gatsby": "next",
    "gatsby-image": "next",
    "gatsby-plugin-catch-links": "next",
    "gatsby-plugin-glamor": "next",
    "gatsby-plugin-sass": "next",
    "gatsby-plugin-sharp": "next",
    "gatsby-remark-autolink-headers": "next",
    "gatsby-remark-copy-linked-files": "next",
    "gatsby-remark-images": "next",
    "gatsby-remark-katex": "next",
    "gatsby-remark-prismjs": "next",
    "gatsby-remark-responsive-iframe": "next",
    "gatsby-remark-smartypants": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "gatsby-transformer-sharp": "next",
    "gatsby-transformer-yaml": "next",
    "husky": "^0.14.3",
    "lodash-webpack-plugin": "^0.11.2",
    "prettier": "^1.7.4",
    "prettier-eslint-cli": "^4.4.0"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "es6-object-assign": "^1.1.0",
    "lodash": "^4.17.4",
    "lodash-id": "^0.14.0",
    "prismjs": "^1.14.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-typography": "^0.15.10",
    "rehype-react": "^3.0.2",
    "slash": "^1.0.0",
    "typeface-space-mono": "^0.0.22",
    "typeface-spectral": "^0.0.29",
    "typography": "^0.15.10"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "dev": "gatsby develop",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "build": "gatsby build",
    "format": "prettier-eslint --write \"**/*.md\" \"*.js\" \"src/**/*.js\" \"src/styles/*css\" --ignore \"public\" \"node_modules\"",
    "lint-staged": "lint-staged",
    "serve": "gatsby serve"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  }
